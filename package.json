{
  "name": "ld-vanilla-template",
  "version": "0.0.1",
  "description": "template for vanilla javascript project",
  "homepage": "https://bigbuggames.github.io/ld-vanilla-template",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint src/*.ts src/**/*.ts",
    "check-types": "tsc",
    "clean": "rimraf ./dist",
    "start": "node server.js",
    "build:dev": "webpack --config ./webpack/webpack.dev.js",
    "publish": "npm run clean && npm run build:dev && gh-pages -d dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint && npm test",
      "pre-push": "npm run lint && npm test"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-computed-properties": "^7.2.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-stage-2": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@testing-library/dom": "^6.4.1",
    "@types/jest": "^24.0.18",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "core-js": "^2.6.9",
    "css-loader": "^3.2.0",
    "directory-named-webpack-plugin": "^4.0.1",
    "eslint": "^6.5.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-jest": "^22.17.0",
    "express": "^4.17.1",
    "gh-pages": "^2.1.1",
    "howler": "^2.1.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.7",
    "jest": "^24.9.0",
    "linaria": "^1.3.1",
    "mini-css-extract-plugin": "^0.8.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "rimraf": "^3.0.0",
    "seedrandom": "^3.0.5",
    "typescript": "^3.6.3",
    "typescript-eslint": "0.0.1-alpha.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2"
  },
  "browserslist": "> 0.25%, not dead"
}
